generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Session {
  id        String   @id @default(cuid())
  code      String   @db.Text
  css       String?  @db.Text
  config    String   @db.Text
  createdAt DateTime @default(now())
}
