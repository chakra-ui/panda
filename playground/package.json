{
  "name": "playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "css:codegen": "pnpm panda codegen --silent",
    "types:codegen": "tsx scripts/bundle-types.mts",
    "prepare": "pnpm db:generate && pnpm css:codegen",
    "build": "pnpm types:codegen && next build",
    "start": "next start",
    "lint": "next lint",
    "dev": "next dev",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:push": "prisma db push --skip-generate",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "viz": "ANALYZE=true next build"
  },
  "dependencies": {
    "@ark-ui/react": "0.15.0",
    "@monaco-editor/react": "4.6.0",
    "@pandacss/config": "workspace:*",
    "@pandacss/generator": "workspace:*",
    "@pandacss/parser": "workspace:*",
    "@pandacss/preset-base": "workspace:*",
    "@pandacss/preset-panda": "workspace:*",
    "@prisma/client": "5.1.0",
    "@textea/json-viewer": "3.2.3",
    "hookable": "5.5.3",
    "merge-anything": "5.1.7",
    "monaco-editor": "0.40.0",
    "monaco-jsx-syntax-highlight": "^1.2.0",
    "nanoid": "4.0.2",
    "next": "14.0.3",
    "next-themes": "^0.2.1",
    "os-browserify": "0.3.0",
    "outdent": " ^0.8.0",
    "path-browserify": "1.0.1",
    "prettier": "^2.8.8",
    "prism-react-renderer": "2.0.6",
    "prisma": "5.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-live-runner": "^1.0.5",
    "ts-pattern": "5.0.5",
    "typescript": "5.3.3",
    "usehooks-ts": "2.9.1",
    "util": "0.12.5",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.1",
    "@pandacss/dev": "workspace:*",
    "@pandacss/types": "workspace:*",
    "@types/node": "20.4.5",
    "@types/prettier": "2.7.3",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.17",
    "dts-bundle-generator": "^8.1.2",
    "eslint": "8.46.0",
    "eslint-config-next": "13.5.6",
    "raw-loader": "^4.0.2",
    "tsx": "3.12.10"
  }
}
