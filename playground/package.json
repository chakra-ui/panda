    "os-browserify": "0.3.0",
    "outdent": " ^0.8.0",
    "path-browserify": "1.0.1",
    "prism-react-renderer": "2.4.1",
    "prisma": "5.22.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-live-runner": "1.0.7",
    "sucrase": "3.35.0",
