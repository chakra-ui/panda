{
  "name": "playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "css:codegen": "pnpm panda codegen --silent",
    "types:codegen": "tsx scripts/bundle-types.mts",
    "prepare": "pnpm db:generate && pnpm css:codegen",
    "build": "pnpm types:codegen && next build",
    "start": "next start",
    "lint": "next lint",
    "dev": "next dev",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:push": "prisma db push --skip-generate",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "viz": "ANALYZE=true next build"
  },
  "dependencies": {
    "@ark-ui/anatomy": "2.2.0",
    "@ark-ui/react": "2.1.1",
    "@monaco-editor/react": "4.6.0",
    "@pandacss/config": "workspace:*",
    "@pandacss/generator": "workspace:*",
    "@pandacss/parser": "workspace:*",
    "@pandacss/preset-base": "workspace:*",
    "@pandacss/preset-panda": "workspace:*",
    "@prisma/client": "5.9.1",
    "@textea/json-viewer": "3.4.0",
    "lightningcss-wasm": "1.23.0",
    "merge-anything": "5.1.7",
    "monaco-editor": "0.46.0",
    "monaco-editor-auto-typings": "0.4.5",
    "monaco-jsx-syntax-highlight": "1.2.0",
    "nanoid": "5.0.5",
    "next": "14.1.0",
    "next-themes": "0.2.1",
    "os-browserify": "0.3.0",
    "outdent": " ^0.8.0",
    "path-browserify": "1.0.1",
    "prism-react-renderer": "2.3.1",
    "prisma": "5.9.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-live-runner": "1.0.5",
    "sucrase": "3.35.0",
    "ts-pattern": "5.0.8",
    "typescript": "5.3.3",
    "usehooks-ts": "2.14.0",
    "util": "0.12.5",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.1.0",
    "@pandacss/dev": "workspace:*",
    "@pandacss/types": "workspace:*",
    "@types/prettier": "2.7.3",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "dts-bundle-generator": "9.3.1",
    "eslint-config-next": "14.1.0",
    "raw-loader": "4.0.2",
    "tsx": "4.7.1"
  }
}
