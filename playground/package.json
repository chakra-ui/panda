{
  "name": "playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "css:codegen": "pnpm panda codegen --silent",
    "types:codegen": "tsx scripts/bundle-types.mts",
    "prepare": "pnpm db:generate && pnpm css:codegen",
    "build": "pnpm types:codegen && next build",
    "start": "next start",
    "lint": "eslint .",
    "dev": "next dev",
    "db:format": "prisma format",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset",
    "viz": "ANALYZE=true next build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ark-ui/react": "5.26.2",
    "@monaco-editor/react": "4.7.0",
    "@pandacss/config": "workspace:*",
    "@pandacss/generator": "workspace:*",
    "@pandacss/logger": "workspace:*",
    "@pandacss/parser": "workspace:*",
    "@pandacss/preset-base": "workspace:*",
    "@pandacss/preset-panda": "workspace:*",
    "@prisma/client": "5.22.0",
    "@textea/json-viewer": "4.0.1",
    "lightningcss-wasm": "1.30.2",
    "merge-anything": "5.1.7",
    "monaco-editor": "0.54.0",
    "monaco-editor-auto-typings": "0.4.6",
    "monaco-jsx-syntax-highlight": "1.2.2",
    "nanoid": "5.1.6",
    "next": "15.5.6",
    "next-themes": "0.4.6",
    "os-browserify": "0.3.0",
    "outdent": " ^0.8.0",
    "path-browserify": "1.0.1",
    "prism-react-renderer": "2.4.1",
    "prisma": "5.22.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-live-runner": "1.0.7",
    "sucrase": "3.35.0",
    "ts-pattern": "5.9.0",
    "typescript": "5.9.3",
    "usehooks-ts": "2.16.0",
    "util": "0.12.5",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@next/bundle-analyzer": "15.5.6",
    "@pandacss/dev": "workspace:*",
    "@pandacss/types": "workspace:*",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "dts-bundle-generator": "9.5.1",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.5.6",
    "eslint-plugin-react-hooks": "^7.0.0",
    "raw-loader": "4.0.2",
    "tsx": "4.20.6"
  },
  "browserslist": [
    "Chrome 118"
  ]
}
