{
  "name": "panda-css-extension",
  "displayName": "panda-css-extension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "panda-css-extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsup src/extension.ts --format cjs --external vscode --minify -d out",
    "dev": "tsup src/extension.ts --watch --format cjs --external vscode -d out",
    "clean": "rm -rf dist && rm -rf out && rm -rf .vscode-test",
    "lint": "eslint ./src --ext .ts,.tsx --fix",
    "pretest": "tsc -p ./ && rm -f .vscode-test/user-",
    "test": "node ./src/tests/runTest.ts",
    "deploy": "vsce publish --no-dependencies",
    "package": "vsce package --pre-release --no-dependencies"
  },
  "devDependencies": {
    "@types/estree": "^1.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@pandacss/node": "workspace:^",
    "acorn": "^8.8.2",
    "acorn-jsx": "^5.3.2",
    "acorn-loose": "^8.3.0",
    "acorn-walk": "^8.2.0",
    "tsup": "6.7.0",
    "vscode": "^1.1.37",
    "vscode-languageclient": "^8.1.0",
    "vscode-languageserver": "^8.1.0",
    "vscode-languageserver-textdocument": "^1.0.8",
    "vscode-uri": "^3.0.7"
  }
}
