{
  "name": "sandbox-codegen",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "test:preact": "MODE=preact vitest run",
    "test:qwik": "MODE=qwik vitest run",
    "test:react": "MODE=react vitest run",
    "test:solid": "MODE=solid vitest run",
    "test:vue": "MODE=vue vitest run",
    "test": "pnpm test:react && pnpm test:preact && pnpm test:qwik && pnpm test:solid && pnpm test:vue",
    "gen:react": "pnpm panda codegen --clean",
    "gen:preact": "pnpm panda codegen --clean --config panda.preact.config.ts",
    "gen:qwik": "pnpm panda codegen --clean --config panda.qwik.config.ts",
    "gen:solid": "pnpm panda codegen --clean --config panda.solid.config.ts",
    "gen:vue": "pnpm panda codegen --clean --config panda.vue.config.ts",
    "prepare": "pnpm gen:react && pnpm gen:preact && pnpm gen:qwik && pnpm gen:solid && pnpm gen:vue",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@pandacss/dev": "workspace:^",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.6",
    "@preact/preset-vite": "2.5.0",
    "@solidjs/testing-library": "^0.8.4",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/preact": "^3.0.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/vue": "^7.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "happy-dom": "^12.1.6",
    "jsdom": "^22.1.0",
    "preact": "^10.16.0",
    "solid-js": "^1.7.12",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "0.33.0",
    "vue": "^3.3.3"
  }
}
