{
  "name": "css-panda",
  "version": "0.0.1",
  "description": "The types for css panda",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Segun Adebayo <joseshegs@gmail.com>",
  "bin": {
    "panda": "dist/cli-default.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./presets": {
      "import": "./dist/presets.mjs",
      "require": "./dist/presets.js",
      "types": "./dist/presets.d.ts"
    },
    "./postcss": "./postcss.js",
    "./astro": "./astro.mjs",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "presets": [
        "./dist/presets.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/cli-*.ts src/presets.ts src/index.ts --format=esm,cjs --dts --no-splitting",
    "build-fast": "tsup src/cli-*.ts src/presets.ts src/index.ts --format=esm,cjs --no-dts --no-splitting",
    "dev": "pnpm build-fast --watch"
  },
  "files": [
    "dist",
    "bin",
    "postcss.js"
  ],
  "dependencies": {
    "@css-panda/config": "workspace:*",
    "@css-panda/error": "workspace:*",
    "@css-panda/logger": "workspace:*",
    "@css-panda/node": "workspace:*",
    "@css-panda/shared": "workspace:*",
    "@css-panda/types": "workspace:*",
    "cac": "6.7.14",
    "vite": "^3.2.0"
  },
  "devDependencies": {
    "@css-panda/presets": "workspace:*",
    "kleur": "^4.1.5"
  }
}
