{
  "name": "@pandacss/dev",
  "version": "0.0.1",
  "description": "The user facing package for panda css",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Segun Adebayo <joseshegs@gmail.com>",
  "bin": {
    "panda": "bin.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./presets": {
      "import": "./dist/presets.mjs",
      "require": "./dist/presets.js",
      "types": "./dist/presets.d.ts"
    },
    "./postcss": "./postcss.js",
    "./astro": {
      "import": "./astro.mjs",
      "types": "./astro.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "presets": [
        "./dist/presets.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/cli-*.ts src/presets.ts src/index.ts --format=esm,cjs --dts --no-splitting --shims",
    "postbuild": "pnpm app:gen",
    "build-fast": "tsup src/cli-*.ts src/presets.ts src/index.ts --format=esm,cjs --no-dts --no-splitting --shims",
    "dev": "pnpm build-fast --watch",
    "css": "dist/cli-default.js",
    "app": "cd app && ../dist/cli-default.js --clean",
    "app:dev": "cd app && ../dist/cli-default.js --watch",
    "app:gen": "cd app && ../dist/cli-default.js codegen --clean"
  },
  "files": [
    "dist",
    "bin",
    "app",
    "bin.js",
    "postcss.js",
    "astro.mjs"
  ],
  "dependencies": {
    "@ark-ui/react": "0.0.0-rc-20230304191834",
    "@astrojs/react": "2.1.0",
    "@monaco-editor/react": "4.4.6",
    "@pandacss/config": "workspace:*",
    "@pandacss/error": "workspace:*",
    "@pandacss/generator": "workspace:*",
    "@pandacss/logger": "workspace:*",
    "@pandacss/node": "workspace:*",
    "@pandacss/parser": "workspace:*",
    "@pandacss/shared": "workspace:*",
    "@pandacss/token-dictionary": "workspace:*",
    "astro": "2.0.12",
    "cac": "6.7.14",
    "crosspath": "^2.0.0",
    "merge-anything": "^5.1.4",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-frame-component": "^5.2.6",
    "react-live": "^3.1.1",
    "usehooks-ts": "2.9.1",
    "util": "^0.12.5",
    "vite": "4.2.1"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@pandacss/presets": "workspace:*",
    "@pandacss/types": "workspace:*",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/update-notifier": "6.0.2",
    "@vitejs/plugin-react": "3.1.0",
    "execa": "7.1.1",
    "kleur": "4.1.5",
    "update-notifier": "6.0.2"
  }
}
