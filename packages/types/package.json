{
  "name": "@pandacss/types",
  "version": "0.0.1",
  "description": "The types for css panda",
  "main": "dist/index.d.ts",
  "author": "Segun Adebayo <joseshegs@gmail.com>",
  "scripts": {
    "prepare": "cp node_modules/csstype/index.d.ts src/csstype.d.ts",
    "build": "tsup index.ts --clean --dts --dts-only",
    "build-fast": "tsup index.ts --dts --dts-only",
    "dev": "chokidar -c 'pnpm build' src --initial"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "csstype": "3.1.2",
    "tsup": "6.7.0"
  },
  "dependencies": {
    "@pandacss/extractor": "workspace:*"
  }
}
