{
  "name": "@css-panda/ast",
  "version": "0.0.1",
  "description": "The static parser for css panda",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Segun Adebayo <joseshegs@gmail.com>",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format=esm,cjs --dts",
    "build-fast": "tsup src/index.ts --format=esm,cjs --no-dts",
    "dev": "pnpm build-fast --watch"
  },
  "dependencies": {
    "@css-panda/logger": "workspace:*",
    "@css-panda/shared": "workspace:*",
    "@css-panda/is-valid-prop": "workspace:*",
    "@css-panda/types": "workspace:*",
    "ts-pattern": "4.0.5",
    "ts-morph": "16.0.0"
  },
  "devDependencies": {
    "@css-panda/fixture": "workspace:*"
  },
  "files": [
    "dist"
  ]
}
