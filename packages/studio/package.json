{
  "name": "@pandacss/studio",
  "version": "0.4.0",
  "description": "The automated token documentation for Panda CSS",
  "main": "dist/studio.js",
  "module": "dist/studio.mjs",
  "types": "dist/studio.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "codegen": "panda",
    "build": "tsup scripts --format=esm,cjs --dts --no-splitting --shims",
    "postbuild": "panda",
    "build-fast": "tsup scripts --format=esm,cjs --no-dts --no-splitting --shims",
    "dev": "pnpm build-fast --watch scripts",
    "dev:panda": "panda --clean --watch",
    "dev:astro": "astro dev",
    "start": "concurrently \"pnpm dev:panda\" \"pnpm dev:astro\""
  },
  "files": [
    "dist",
    "src",
    "public",
    "styled-system",
    "*.ts"
  ],
  "keywords": [
    "panda",
    "studio",
    "design tokens",
    "documentation"
  ],
  "author": "Segun Adebayo <joseshegs@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ark-ui/react": "0.6.0",
    "@astrojs/react": "2.2.1",
    "@pandacss/types": "workspace:*",
    "@pandacss/config": "workspace:*",
    "@pandacss/shared": "workspace:*",
    "@pandacss/token-dictionary": "workspace:*",
    "@pandacss/logger": "workspace:*",
    "@pandacss/node": "workspace:*",
    "astro": "2.6.4",
    "javascript-stringify": "2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "vite": "4.3.9"
  },
  "devDependencies": {
    "@pandacss/dev": "workspace:*",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@vitejs/plugin-react": "4.0.0",
    "execa": "7.1.1"
  }
}
