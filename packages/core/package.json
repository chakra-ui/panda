{
  "name": "@pandacss/core",
  "version": "0.17.0",
  "description": "core functions for extract-it",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Segun Adebayo <joseshegs@gmail.com>",
  "scripts": {
    "build": "tsup --entry src/index.ts --entry src/classname.ts --format=esm,cjs --dts",
    "build-fast": "tsup --entry src/index.ts --entry src/classname.ts --format=esm,cjs --no-dts",
    "dev": "pnpm build-fast --watch"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@pandacss/error": "workspace:*",
    "@pandacss/logger": "workspace:*",
    "@pandacss/shared": "workspace:*",
    "@pandacss/token-dictionary": "workspace:*",
    "@pandacss/types": "workspace:*",
    "autoprefixer": "10.4.15",
    "hookable": "5.5.3",
    "lightningcss": "^1.22.0",
    "lodash.merge": "4.6.2",
    "postcss": "^8.4.31",
    "postcss-nested": "6.0.1",
    "postcss-selector-parser": "^6.0.13",
    "ts-pattern": "5.0.5"
  },
  "devDependencies": {
    "@types/lodash.merge": "4.6.7"
  }
}
