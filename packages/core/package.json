{
  "name": "@css-panda/core",
  "version": "0.0.1",
  "description": "core functions for extract-it",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Segun Adebayo <joseshegs@gmail.com>",
  "scripts": {
    "build": "tsup --entry src/index.ts --entry src/classname.ts --format=esm,cjs --dts",
    "build-fast": "tsup --entry src/index.ts --entry src/classname.ts --format=esm,cjs --no-dts",
    "dev": "pnpm build-fast --watch"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@css-panda/error": "workspace:*",
    "@css-panda/types": "workspace:*",
    "@css-panda/shared": "workspace:*",
    "@css-panda/logger": "workspace:*",
    "@css-panda/tokens": "workspace:*",
    "camelcase-css": "^2.0.1",
    "postcss": "8.4.16",
    "postcss-selector-parser": "6.0.10",
    "postcss-nested": "5.0.6",
    "postcss-sort-media-queries": "4.3.0",
    "postcss-discard-duplicates": "5.1.0",
    "postcss-normalize-whitespace": "5.1.1",
    "postcss-prettify": "0.3.4",
    "autoprefixer": "10.4.8",
    "ts-pattern": "4.0.5",
    "lodash.merge": "4.6.2"
  },
  "devDependencies": {
    "@css-panda/fixture": "workspace:*",
    "@types/lodash.merge": "4.6.7"
  }
}
